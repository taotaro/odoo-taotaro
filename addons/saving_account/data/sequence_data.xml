<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="seq_saving_account" model="ir.sequence">
    <field name="name">Saving Account ID</field>
    <field name="code">saving_account</field>
    <field name="suffix">AC</field>
    <field name="padding">7</field>
    <field name="company_id" eval="False"/>
  </record>

  <record id="seq_saving_account_entry" model="ir.sequence">
    <field name="name">Saving Account Entry No</field>
    <field name="code">saving_account.entry</field>
    <field name="company_id" eval="False"/>
  </record>

</odoo>